<%= form_for venue do |f| %>
    Name: <%= f.text_field :name %>
    Location: <%= f.text_field :location %>
    Crowd Capacity: <%= f.number_field :crowd_capacity %>
    Parking?: Yes <%= check_box("venue", "parking", {}, "yes", "no") %>
    <%= f.submit %>
<% end %>

<%= link_to 'Venues', venues_path %>
