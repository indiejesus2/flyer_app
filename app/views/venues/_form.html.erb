<%= form_for venue do |f| %>
    Name: <%= f.text_field :name %> <br>
    City: <%= f.text_field :city %> State: <%= f.text_field :state %> <br>
    Crowd Capacity: <%= f.number_field :crowd_capacity %> <br>
    Parking?: Yes <%= check_box("venue", "parking", {}, "true", "false") %> <br>
    <%= f.submit %>
<% end %>